{
    "name": "hnhdigital-os/laravel-datasets",
    "description": "Provides console commands, models and migration scripts to syncronize datasets.",
    "keywords": ["laravel","datasets"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@hnh.digital"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.1",
        "league/flysystem": "^1.0",
        "league/csv": "^9.0",
        "hnhdigital-os/laravel-datasets-okfn": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "illuminate/database": "4.*|5.*",
        "symfony/process": "~2.3",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "HnhDigital\\Datasets\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HnhDigital\\Datasets\\ServiceProvider"
            ]
        }
    }
}
